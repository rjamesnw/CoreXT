{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "outDir": "wwwroot/js",
    "rootDir": "Scripts"
  },
  "include": [
    "typings/**/*.ts",
    "Scripts/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "wwwroot/**/*.ts",
    "Scripts/corext.ts",
    "Scripts/table.ts"
  ]
}
